{
    "git": {
        "deploymentEnabled": true
    },
    "framework": "nextjs",
    "functions": {
        "api/**/*.js": {
            "maxDuration": 30
        }
    },
    "headers": [
        {
            "source": "/files/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600"
                }
            ]
        }
    ]
}